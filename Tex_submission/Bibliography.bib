issn = {1744-8603},
journal = {Globalization and Health},
keywords = {Adult,Cost of Illness,Diabetes Complications,Diabetes Complications: economics,Diabetes Complications: epidemiology,Diabetes Complications: therapy,Diabetes Mellitus,Diabetes Mellitus: economics,Diabetes Mellitus: epidemiology,Diabetes Mellitus: therapy,Female,Health Policy,Humans,Male,Mexico,Mexico: epidemiology,Middle Aged},
