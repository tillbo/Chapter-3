pages = {98--112},
pmid = {24981505},
publisher = {Elsevier B.V.},
title = {{The heterogeneous effects of HIV testing}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0167629614000836},
volume = {37},
year = {2014}
}
