isbn = {0085-2538 (Print)$\backslash$n0085-2538 (Linking)},
issn = {00852538},
journal = {Kidney International},
keywords = {End-stage renal disease,Insulin,Macroalbuminuria,Microalbuminuria,Nephropathy,Plasma creatinine,Type 2 diabetes},
