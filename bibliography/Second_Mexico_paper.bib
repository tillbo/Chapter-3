  issn = {1467-789X},
  journal = {Obesity reviews: an official journal of the International Association for the Study of Obesity},
  keywords = {Adult,Child,Developing Countries,Developing Countries: economics,Developing Countries: statistics \& numerical data,Female,Humans,Income,Male,Obesity,Obesity: economics,Obesity: epidemiology,Social Class,Socioeconomic Factors},
  number = {11},
