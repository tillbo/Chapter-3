  keywords = {Adolescent,Adult,Beverages,Beverages: adverse effects,Child,Diabetes Mellitus,Energy Intake,Female,Food Habits,Humans,Male,Mexico,Middle Aged,Nutrition Surveys,Obesity,Obesity: etiology,Socioeconomic Factors,Type 2,Type 2: etiology,Young Adult},
  number = {12},
