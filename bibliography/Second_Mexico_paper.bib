  isbn = {0098-7484},
  issn = {1538-3598},
  journal = {JAMA : the journal of the American Medical Association},
  keywords = {Aged,Blood Glucose,Body Mass Index,Counseling,Diabetes Mellitus, Type 2,Diabetes Mellitus, Type 2: therapy,Diet,Exercise,Female,Humans,Intervention Studies,Life Style,Male,Middle Aged,Overweight,Patient Education as Topic,Psychotherapy, Group,Remission Induction,Risk Reduction Behavior,Social Support,Treatment Outcome,Weight Loss},
  number = {23},
  pages = {2489--96},
  pmid = {23288372},
  title = {{Association of an intensive lifestyle intervention with remission of type 2 diabetes.}},
  url = {http://www.ncbi.nlm.nih.gov/pubmed/23288372},
  volume = {308},
  year = {2012},
