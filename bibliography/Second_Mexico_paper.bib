  keywords = {Employment,Endogeneity,Spinal cord injury},
  number = {2},
  pages = {223--232},
  pmid = {20605751},
  publisher = {Elsevier B.V.},
  title = {{Labour market outcomes for people with a spinal cord injury}},
  url = {http://dx.doi.org/10.1016/j.ehb.2010.05.003},
